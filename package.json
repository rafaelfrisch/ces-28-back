    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "helmet": "^3.21.3",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
