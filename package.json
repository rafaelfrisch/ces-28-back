{
  "name": "api-cedro",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel ./src --out-dir build && node ./build/bin/www",
    "startdev": "nodemon --exec babel-node ./src/bin/www",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**' '!build/**' '!coverage/**' '!.nyc_output/**'",
    "postpretty": "yarn lint --fix",
    "test": "env-cmd -f .env jest --watchAll",
    "createAdmin": "babel-node ./src/queries/commands createAdmin",
    "createOrders": "babel-node ./src/queries/commands createOrders",
    "createProducts": "babel-node ./src/queries/commands createProducts"
  },
  "dependencies": {
    "@babel/cli": "^7.8.3",
